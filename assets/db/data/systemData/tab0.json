{
  "name": "main",
  "tab": {
    "flutterType": "BottomNavigationBarItem",
    "icon": {
      "flutterType": "Icon",
      "src": "crop_5_4"
    },
    "label": "Home"
  },
  "content": {
    "data": {
      "uuid": "fe631169-a194-4eb5-a9f0-499c9710863e",
      "type": "userDataRSync"
    },
    "flutterType": "Scaffold",
    "body": {
      "flutterType": "CustomScrollView",
      "onStateDataUpdate": true,
      "padding": "10,20,10,0",
      "appBar": {
        "flutterType": "SliverAppBar",
        "title": {
          "flutterType": "Text",
          "label": "${state(main,title,)}",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 17
          }
        }
      },
      "children": [
        {
          "ChildrenExtension": "Iterator",
          "dataType": "state",
          "key": "listItem",
          "theme": "ButtonGroup",
          "extendDataElement": {
            "templateWidgetSrc": "${state(main,theme,ListSimple)}",
            "onTap": {
              "sysInvoke": "NavigatorPush",
              "args": {
                "type": "bottomSheet",
                "height": 200,
                "link": {
                  "template": "GameCode.json"
                }
              }
            }
          },
          "compileTemplateList": [
            "extendDataElement.templateWidgetSrc"
          ]
        }
      ]
    },
    "constructor": {
      "jsRouter": "ListItem.ai.js",
      "args": {
        "method": "constructor"
      }
    },
    "onActive": {
      "jsRouter": "ListItem.ai.js",
      "args": {
        "method": "constructor"
      }
    }
  }
}