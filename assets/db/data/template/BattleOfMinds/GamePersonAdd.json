{
  "flutterType": "Padding",
  "padding": 15,
  "child": {
    "flutterType": "Row",
    "children": [
      {
        "flutterType": "Expanded",
        "child": {
          "flutterType": "TextField",
          "keyboardType": "text",
          "autofocus": true,
          "onChangedSetStateNotify": false,
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 17
          },
          "name": "Name",
          "decoration": {
            "flutterType": "InputDecoration",
            "hintText": "Имя нового участника",
            "contentPadding": "15,0,15,0",
            "filled": true,
            "fillColor": "schema:onBackground",
            "border": {
              "flutterType": "OutlineInputBorder"
            },
            "suffixIconConstraints": {
              "flutterType": "BoxConstraints",
              "minHeight": 44,
              "maxHeight": 44
            },
            "suffixIcon": {
              "flutterType": "IconButton",
              "padding": 0,
              "icon": {
                "flutterType": "Icon",
                "color": "grey",
                "src": "clear",
                "size": 18
              },
              "onPressed": {
                "sysInvoke": "Controller",
                "args": {
                  "controller": "Name",
                  "case": "reset"
                }
              }
            },
            "enabledBorder": {
              "flutterType": "OutlineInputBorder",
              "borderRadius": 10,
              "borderSide": {
                "flutterType": "BorderSide",
                "color": "transparent",
                "width": 0
              }
            },
            "focusedBorder": {
              "flutterType": "OutlineInputBorder",
              "borderRadius": 10,
              "borderSide": {
                "flutterType": "BorderSide",
                "color": "transparent",
                "width": 0
              }
            }
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "width": 15
      },
      {
        "flutterType": "RawMaterialButton",
        "fillColor": "schema:projectPrimary",
        "padding": 10,
        "onPressed": {
          "jsRouter": "SecretConnections/GameTeam.ai.js",
          "args": {
            "method": "addPerson"
          }
        },
        "child": {
          "flutterType": "Icon",
          "src": "arrow_upward",
          "color": "schema:projectPrimaryText",
          "size": 20
        }
      }
    ]
  }
}