{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "crossAxisAlignment": "start",
    "children": [
      {
        "flutterType": "State",
        "key": "gridWord"
      },
      {
        "flutterType": "SizedBox",
        "height": 5
      },
      {
        "flutterType": "Visibility",
        "visible": "${state(visibleGenerateWord, false)}",
        "child": {
          "flutterType": "Button",
          "minimumSize": "9999,44",
          "backgroundColor": "blue",
          "height": 44,
          "onPressed": {
            "jsInvoke": "GameGenerateWord.js",
            "args": {
              "includeStateData": true,
              "includePageArgument": true,
              "switch": "generateWord"
            }
          },
          "child": {
            "flutterType": "Text",
            "label": "Сгенерировать слова",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 14,
              "color": "white"
            }
          }
        }
      }
    ]
  }
}
