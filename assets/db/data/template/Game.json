{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "CustomScrollView",
    "includeBottomOffset": true,
    "onStateDataUpdate": true,
    "appBar": {
      "flutterType": "SliverAppBar",
      "title": {
        "flutterType": "Text",
        "label": "CODENAMES",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        }
      }
    },
    "children": [
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "gridWord",
        "template": {
          "SliverGroup": {
            "crossAxisCount": 5,
            "childAspectRatio": 3,
            "mainAxisSpacing": 5,
            "crossAxisSpacing": 5,
            "padding": "20,10,20,0",
            "type": "grid",
            "name": "gridWord"
          },
          "flutterType": "Container",
          "color": "red",
          "child": {
            "flutterType": "Align",
            "textAlign": "center",
            "child": {
              "flutterType": "Text",
              "label": "${label}"
            }
          }
        }
      },
      {
        "ChildrenExtension": "StateDataSwitch",
        "key": "SwitchKey",
        "children": [
          {
            "case": "default",
            "flutterType": "Template",
            "src": "GameEnterName"
          },
          {
            "case": "word",
            "flutterType": "Template",
            "src": "GameSelectWord"
          },
          {
            "case": "run",
            "flutterType": "Template",
            "src": "GameRun"
          },
          {
            "case": "finish",
            "flutterType": "Template",
            "src": "GameFinish"
          }
        ]
      }
    ]
  },
  "onRenderFloatingActionButton": {
    "jsInvoke": "Game.js",
    "args": {
      "includeStateData": true,
      "includePageArgument": true,
      "switch": "onRenderFloatingActionButton"
    }
  }
}