{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "crossAxisAlignment": "start",
    "children": [
      {
        "flutterType": "State",
        "key": "gridWord"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Visibility",
        "visible": "${state(toNextTeam, false)}",
        "child": {
          "flutterType": "Button",
          "minimumSize": "9999,44",
          "backgroundColor": "blue",
          "height": 44,
          "onPressed": {
            "sysInvoke": "Alert",
            "args": {
              "confirmAction": true,
              "onPressed": {
                "jsInvoke": "Game.js",
                "args": {
                  "includeStateData": true,
                  "includePageArgument": true,
                  "switch": "toNextTeam"
                }
              }
            }
          },
          "child": {
            "flutterType": "Text",
            "label": "Следующий ход",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 14,
              "color": "white"
            }
          }
        }
      }
    ]
  }
}