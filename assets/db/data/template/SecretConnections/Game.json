{
  "flutterType": "Scaffold",
  "body": {
    "flutterType": "CustomScrollView",
    "onStateDataUpdate": true,
    "padding": "10,20,10,0",
    "appBar": {
      "flutterType": "SliverAppBar",
      "title": {
        "flutterType": "Text",
        "label": "Тайные Связи",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        }
      }
    },
    "children": [
      {
        "ChildrenExtension": "StateDataSwitch",
        "key": "switchKey",
        "children": [
          {
            "case": "team",
            "flutterType": "Template",
            "src": "GameTeam"
          },
          {
            "case": "word",
            "flutterType": "Template",
            "src": "GameWord"
          },
          {
            "case": "run",
            "flutterType": "Template",
            "src": "GameRun"
          },
          {
            "case": "finish",
            "flutterType": "Template",
            "src": "GameFinish"
          },
          {
            "case": "error",
            "flutterType": "Template",
            "src": "GameError"
          }
        ]
      }
    ]
  }
}