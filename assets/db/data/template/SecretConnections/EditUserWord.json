{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "mainAxisSize": "max",
    "children": [
      {
        "flutterType": "Row",
        "children": [
          {
            "flutterType": "Expanded",
            "child": {
              "flutterType": "Text",
              "label": "${pageArgs(selected.label,?)}"
            }
          },
          {
            "flutterType": "RawMaterialButton",
            "padding": 10,
            "fillColor": "blue",
            "onPressed": {
              "jsRouter": "SecretConnections/EditUserWord.ai.js",
              "args": {
                "method": "add"
              }
            },
            "child": {
              "flutterType": "Icon",
              "src": "save",
              "color": "white",
              "size": 20
            }
          },
          {
            "flutterType": "SizedBox",
            "width": 20
          },
          {
            "flutterType": "RawMaterialButton",
            "padding": 10,
            "fillColor": "blue",
            "onPressed": {
              "jsRouter": "SecretConnections/EditUserWord.ai.js",
              "args": {
                "method": "add"
              }
            },
            "child": {
              "flutterType": "Icon",
              "src": "add",
              "color": "white",
              "size": 20
            }
          }
        ]
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Expanded",
        "child": {
          "flutterType": "CustomScrollView",
          "isOpacityBottomNavigationBar": false,
          "onStateDataUpdate": true,
          "children": [
            {
              "ChildrenExtension": "Iterator",
              "dataType": "state",
              "key": "listItem",
              "extendDataElement": {
                "template": {
                  "flutterType": "Margin",
                  "margin": "0,0,0,10",
                  "child": {
                    "flutterType": "Row",
                    "children": [
                      {
                        "flutterType": "Expanded",
                        "child": {
                          "flutterType": "Template",
                          "src": "Input",
                          "context": {
                            "key": "Item",
                            "data": {
                              "data": "${label}",
                              "key": "field_${iteratorIndex}"
                            }
                          }
                        }
                      },
                      {
                        "flutterType": "SizedBox",
                        "width": 20
                      },
                      {
                        "flutterType": "RawMaterialButton",
                        "fillColor": "red",
                        "padding": 10,
                        "onPressed": {
                          "jsRouter": "SecretConnections/EditUserWord.ai.js",
                          "args": {
                            "method": "remove",
                            "removeIndex": "${iteratorIndex}"
                          }
                        },
                        "child": {
                          "flutterType": "Icon",
                          "src": "delete_outline",
                          "color": "white",
                          "size": 20
                        }
                      }
                    ]
                  }
                },
                "dataContext": true,
                "compileTemplateList": [
                  "template.child.children.0.child.context.data.data",
                  "template.child.children.0.child.context.data.key",
                  "template.child.children.2.onPressed.args.removeIndex"
                ]
              }
            },
            {
              "flutterType": "SizedBox",
              "height": 20
            }
          ]
        }
      }
    ]
  }
}