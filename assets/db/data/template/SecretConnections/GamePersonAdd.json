{
  "flutterType": "Container",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "children": [
      {
        "flutterType": "Text",
        "label": "Новый участник:"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "TextField",
        "keyboardType": "text",
        "autofocus": true,
        "onChangedSetStateNotify": false,
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        },
        "name": "Name",
        "decoration": {
          "flutterType": "InputDecoration",
          "hintText": "Имя",
          "contentPadding": "15,0,15,0",
          "filled": true,
          "fillColor": "schema:onBackground",
          "border": {
            "flutterType": "OutlineInputBorder"
          },
          "suffixIconConstraints": {
            "flutterType": "BoxConstraints",
            "minHeight": 44,
            "maxHeight": 44
          },
          "suffixIcon": {
            "flutterType": "IconButton",
            "padding": 0,
            "icon": {
              "flutterType": "Icon",
              "color": "grey",
              "src": "clear",
              "size": 18
            },
            "onPressed": {
              "sysInvoke": "Controller",
              "args": {
                "controller": "Name",
                "case": "reset"
              }
            }
          },
          "enabledBorder": {
            "flutterType": "OutlineInputBorder",
            "borderRadius": 10,
            "borderSide": {
              "flutterType": "BorderSide",
              "color": "transparent",
              "width": 0
            }
          },
          "focusedBorder": {
            "flutterType": "OutlineInputBorder",
            "borderRadius": 10,
            "borderSide": {
              "flutterType": "BorderSide",
              "color": "transparent",
              "width": 0
            }
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Button",
        "height": 44,
        "backgroundColor": "blue",
        "onPressed": {
          "jsInvoke": "SecretConnections/GameTeam.js",
          "args": {
            "includeAll": true,
            "method": "addPerson"
          }
        },
        "child": {
          "flutterType": "Text",
          "label": "Добавить",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 14,
            "color": "white"
          }
        }
      }
    ]
  }
}