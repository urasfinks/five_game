{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "crossAxisAlignment": "start",
    "children": [
      {
        "flutterType": "Text",
        "label": "Моё имя в игре: "
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "TextField",
        "keyboardType": "text",
        "data": "${state(deviceName)}",
        "onChangedNotify": false,
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 17
        },
        "name": "Name",
        "decoration": {
          "flutterType": "InputDecoration",
          "hintText": "Имя",
          "contentPadding": "15,0,15,0",
          "filled": true,
          "fillColor": "schema:onBackground",
          "border": {
            "flutterType": "OutlineInputBorder"
          },
          "suffixIconConstraints": {
            "flutterType": "BoxConstraints",
            "minHeight": 44,
            "maxHeight": 44
          },
          "suffixIcon": {
            "flutterType": "IconButton",
            "padding": 0,
            "icon": {
              "flutterType": "Icon",
              "color": "grey",
              "src": "clear",
              "size": 18
            },
            "onPressed": {
              "sysInvoke": "ResetTextController",
              "args": {
                "key": "Name"
              }
            }
          },
          "enabledBorder": {
            "flutterType": "OutlineInputBorder",
            "borderRadius": 10,
            "borderSide": {
              "flutterType": "BorderSide",
              "color": "transparent",
              "width": 0
            }
          },
          "focusedBorder": {
            "flutterType": "OutlineInputBorder",
            "borderRadius": 10,
            "borderSide": {
              "flutterType": "BorderSide",
              "color": "transparent",
              "width": 0
            }
          }
        },
        "onEditingComplete": {
          "jsInvoke": "Game.js",
          "args": {
            "includeStateData": true,
            "includePageArgument": true,
            "switch": "setName"
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Text",
        "label": "Участники игры: "
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listPersonUndefined",
        "theme": "ButtonGroup"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listPersonRed",
        "theme": "ButtonGroup"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listPersonBlue",
        "theme": "ButtonGroup"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Button",
        "minimumSize": "9999,44",
        "backgroundColor": "schema:onBackground",
        "height": 44,
        "onPressed": {
          "sysInvoke": "Alert",
          "args": {
            "confirm": true,
            "actionLabel": "Перемешать",
            "backgroundColor": "schema:onBackground",
            "actionBackgroundColor": "blue",
            "onPressed": {
              "jsInvoke": "Game.js",
              "args": {
                "includeStateData": true,
                "includePageArgument": true,
                "switch": "randomize"
              }
            }
          }
        },
        "child": {
          "flutterType": "Text",
          "label": "Случайно перемешать",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 14,
            "color": "white"
          }
        }
      }
    ]
  }
}
