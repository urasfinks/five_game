{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "Column",
    "crossAxisAlignment": "start",
    "children": [
      {
        "flutterType": "State",
        "key": "gridWord"
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Button",
        "minimumSize": "9999,44",
        "backgroundColor": "blue",
        "height": 44,
        "onPressed": {
          "jsInvoke": "GameGenerateWord.js",
          "args": {
            "includeStateData": true,
            "includePageArgument": true,
            "switch": "generateWord"
          }
        },
        "child": {
          "flutterType": "Text",
          "label": "Сгенерировать слова",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 14,
            "color": "white"
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Button",
        "minimumSize": "9999,44",
        "backgroundColor": "blue",
        "height": 44,
        "onPressed": {
          "jsInvoke": "Game.js",
          "args": {
            "includeStateData": true,
            "includePageArgument": true,
            "switch": "changeGameState",
            "gameState": "run"
          }
        },
        "child": {
          "flutterType": "Text",
          "label": "Начать игру",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 14,
            "color": "white"
          }
        }
      }
    ]
  }
}
