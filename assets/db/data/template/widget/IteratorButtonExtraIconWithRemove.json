{
  "flutterType": "Slidable",
  "newContext": false,
  "children": [
    {
      "flutterType": "SizedBox",
      "width": 100,
      "child": {
        "flutterType": "Row",
        "mainAxisAlignment": "center",
        "children": [
          {
            "flutterType": "RawMaterialButton",
            "fillColor": "red",
            "padding": 10,
            "onPressed": {
              "jsInvoke": "MyGame.js",
              "args": {
                "switch": "onRemove",
                "id_data": "${id_data}",
                "currentRenderTemplateList": [
                  "id_data"
                ]
              }
            },
            "child": {
              "flutterType": "Icon",
              "src": "delete_outline",
              "color": "white",
              "size": 20
            }
          }
        ]
      }
    }
  ],
  "child": {
    "flutterType": "Margin",
    "margin": "17,13,17,13",
    "child": {
      "flutterType": "Row",
      "children": [
        {
          "flutterType": "Expanded",
          "child": {
            "flutterType": "Text",
            "label": "${label}",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 16
            }
          }
        },
        {
          "flutterType": "Opacity",
          "opacity": "${labelExtra|template()|map(,0,1)}",
          "child": {
            "flutterType": "Container",
            "padding": "7,4,7,4",
            "decoration": {
              "color": "schema:background",
              "flutterType": "BoxDecoration",
              "borderRadius": "8"
            },
            "child": {
              "flutterType": "Text",
              "style": {
                "flutterType": "TextStyle",
                "color": "schema:secondary",
                "fontSize": 12,
                "fontWeight": "bold"
              },
              "label": "${labelExtra|template()}"
            }
          }
        },
        {
          "flutterType": "SizedBox",
          "width": 7
        },
        {
          "flutterType": "Icon",
          "src": "${iconSrc,arrow_forward_ios_sharp}",
          "size": "${iconSize,17}",
          "color": "${iconColor,schema:secondary}"
        }
      ]
    }
  }
}